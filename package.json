  "jest": {
    "verbose": true,
    "automock": false,
    "setupFiles": [
      "./src/setupJest.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "scripts": {
    "test": "jest",
    "relay": "node ./data/gqlSetup.js > src/gqlHelper.js && relay-compiler --verbose --src src --exclude '**/generated/**' --schema ./data/schema.json --extensions js jsx",
    "schema": "node ./data/getSchema",
    "params": "node ./data/getTexts > src/params.js",
    "eslint": "./node_modules/.bin/eslint --ext js --ext jsx --ignore-pattern '**/__generated__/**' --ignore-pattern '**/*.min.js' --ignore-pattern '**/setupJest.js' --ignore-pattern '**/gqlHelper.js' --fix src data",
    "elint": "./node_modules/.bin/eslint --ext js --ext jsx --ignore-pattern '**/__generated__/**' --ignore-pattern '**/*.min.js' --ignore-pattern '**/setupJest.js'",
    "storybook": "STORYBOOK_PROJECT_ID=search start-storybook -p 9001 -c .storybook",
    "stylelint": "stylelint 'src/**/*.less' 'src/**/*.css' --config .stylelintrc.js --fix",
    "sanity-check": "node ./sanity-check",
    "format": "prettier --write ."
  },
  "graphql": {
    "file": "./data/schema.json"
  },
